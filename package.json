{
  "name": "@bet-ground/prisma-library",
  "version": "1.0.0",
  "description": "Reusable Prisma library for bet-ground applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "node -e \"const lib = require('./dist/index.js'); console.log('âœ… Library exports working correctly');\"",
    "prisma:clean": "rm -rf node_modules/.prisma && rm -rf prisma/node_modules/.prisma",
    "prisma:main:pull": "prisma db pull --schema=prisma/main/schema.prisma",
    "prisma:main:generate": "prisma generate --schema=prisma/main/schema.prisma",
    "prisma:logs:pull": "prisma db pull --schema=prisma/logs_db/schema.prisma",
    "prisma:logs:generate": "prisma generate --schema=prisma/logs_db/schema.prisma",
    "prisma:stats:pull": "prisma db pull --schema=prisma/statistics/schema.prisma",
    "prisma:stats:generate": "prisma generate --schema=prisma/statistics/schema.prisma",
    "prisma:pull": "npm run prisma:clean && npm run prisma:main:pull && npm run prisma:logs:pull && npm run prisma:stats:pull",
    "prisma:generate": "npm run prisma:clean && npm run prisma:main:generate && npm run prisma:logs:generate && npm run prisma:stats:generate",
    "prisma:full": "npm run prisma:pull && npm run prisma:generate"
  },
  "keywords": [
    "prisma",
    "database",
    "nestjs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/config": "^4.0.2",
    "@prisma/client": "^6.12.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.2.6",
    "express": "^4.18.2",
    "express-useragent": "^1.0.15",
    "jsdom": "^26.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "request-ip": "^3.3.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^22.10.7",
    "prisma": "^6.12.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.4",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.4",
    "@prisma/client": "^6.12.0",
    "axios": "^1.6.0",
    "class-validator": "^0.14.2",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.2.6",
    "express": "^4.18.2",
    "express-useragent": "^1.0.15",
    "jsdom": "^26.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "request-ip": "^3.3.0",
    "socket.io": "^4.8.1"
  }
}
